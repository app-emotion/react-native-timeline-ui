{
  "name": "react-native-awesome-timeline-app",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.0.0",
  "homepage": "https://react-native-awesome-timeline.site",
  "repository": "https://github.com/jeongshin/react-native-awesome-timeline",
  "author": "jeongshin (https://github.com/jeongshin)",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky install",
    "lint": "eslint 'packages/**/*.{ts,tsx}' --ext .ts,.tsx --fix",
    "typescript": "tsc --noemit",
    "patch": "yarn build && cd lib && release-it patch",
    "minor": "yarn build && cd lib && release-it minor",
    "major": "yarn build && cd lib && release-it major",
    "build": "tsc --project tsconfig.build.json",
    "build-docs": "cd docs && yarn deploy",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "jest": "jest --runInBand",
    "test": "jest",
    "build-static-webapp": "expo export:web"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "date-fns": "^2.30.0",
    "expo": "^47.0.0",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.8",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-web": "~0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@expo/webpack-config": "^0.17.2",
    "@jest/types": "^27.5.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@storybook/addon-actions": "^6.5",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^6.5",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/addon-react-native-web": "^0.0.19",
    "@storybook/react-native": "^5.3.27",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-jest": "^29.4.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-react-native-web": "^0.18.12",
    "babel-preset-expo": "^9.2.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-universe": "^11.2.0",
    "husky": "^8.0.2",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "react-native-builder-bob": "^0.20.3",
    "react-test-renderer": "^18.1.0",
    "release-it": "^15.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "packages/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "@types/react": "^18.0.21",
    "@types/react-native": "0.70.6"
  },
  "license": "MIT"
}
